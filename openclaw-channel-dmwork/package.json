{
  "name": "openclaw-channel-dmwork",
  "version": "0.1.0",
  "description": "DMWork channel plugin for OpenClaw via WuKongIM WebSocket",
  "main": "index.ts",
  "type": "module",
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "ws": "^8.16.0",
    "wukongimjssdk": "^1.3.4",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "openclaw": "2026.2.12",
    "typescript": "^5.4.0"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "dmwork",
      "label": "DMWork",
      "selectionLabel": "DMWork (WuKongIM)",
      "docsLabel": "dmwork",
      "blurb": "WuKongIM gateway for DMWork",
      "order": 90
    },
    "install": {
      "localPath": "extensions/dmwork",
      "defaultChoice": "local"
    }
  }
}
